{
  "name": "@j-l/request",
  "version": "1.0.0-beta.1",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "description": "浏览器端请求库",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "author": {
    "name": "章生",
    "email": "me@jiluo.cc",
    "url": "https://www.jiluo.cc"
  },
  "keywords": [
    "http",
    "request",
    "ajax",
    "axios",
    "fetch"
  ],
  "tsup": {
    "entry": [
      "source/index.ts"
    ],
    "splitting": false,
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs",
      "iife"
    ],
    "minify": true
  }
}
